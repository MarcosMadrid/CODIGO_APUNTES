<form class="form" method="post">
    <input placeholder="direccion" name="direccion">
    <button type="submit" class="btn btn-success">Realizar Envio</button>

<ul class="list-group">
    @for(int i = 0; i< 10; i++)
    {
        <li class="list-group-item">
            <input type="checkbox" name="producto" class="form-check" value="Producto @i"/>
            Producto @i
        </li>        
    }
</ul>
</form>